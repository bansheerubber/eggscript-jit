printNumber(5 - -1);
printNumber(5 * 3 - (1 + (35 / 2)));

%a = 5;
%b = 3;
%c = 1;
%d = 35;
%e = 2;
printNumber(%a * %b - (%c + (%d / %e)));

function a(): number {
	return 5.0;
}

function b(): number {
	return 3.0;
}

function c(): number {
	return 1.0;
}

function d(): number {
	return 35.0;
}

function e(): number {
	return 2.0;
}

printNumber(a() * b() - (c() + (d() / e())) / -5.0);

printNumber(-a() * 3.0);

printNumber(-%a * 2);
